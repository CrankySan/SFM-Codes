NAME "AE2 Automation"

-- Energy Distribution
Every 20 ticks do
    input fe:: from cube south side
    output fe:: to centrifuge top side
    output fe:: to siliconpress top side
    output fe:: to calculationpress top side
    output fe:: to calculationprocessor top side
    output fe:: to logicpress top side
    output fe:: to logicprocessor top side
    output fe:: to engineeringpress top side
    output fe:: to engineeringprocessor top side
    output fe:: to concurrentpress top side
    output fe:: to concurrentprocessor top side
    output fe:: to energypress top side
    output fe:: to energyprocessor top side
    output fe:: to accumulationpress top side
    output fe:: to accumulationprocessor top side
    output fe:: to furnace top side
end

--Silicon to Siliconpress
Every 20 ticks do
    Input silicon from silicon
    Output retain 2 silicon to siliconpress round robin by block
End

--Redstone to Processor Inscribers

Every 20 ticks do
    Input "redstone" from "redstone"
    Output retain 2 to calculationprocessor round robin by block slots 2
    Output retain 2 to logicprocessor round robin by block slots 2
    Output retain 2 to engineeringprocessor round robin by block slots 2
    Output retain 2 to concurrentprocessor round robin by block slots 2
    Output retain 2 to energyprocessor round robin by block slots 2
End

-- Certus Dust smelting to Silicon
Every 20 ticks do
input from certusdust
output to furnace top side
end

Every 20 ticks do
input from furnace bottom side
output to silicon
end

-- Fluix Dust to Processor Inscriber
Every 20 ticks do
    input from fluixdust
    output to accumulationprocessor slots 2
end

--Calculation Processor
Every 20 ticks do
    Input certus_quartz_crystal from "certusquartzcrystal"
    Output to calculationpress slots 2
End

Every 20 ticks do
    Input calculation_processor from calculationprocessor
    Output to "output"
End

--Logic Processor
Every 20 ticks do
    Input gold_ingot from "gold"
    Output to logicpress slots 2
End

Every 20 ticks do
    Input logic_processor from logicprocessor
    Output to "output"
End

--Engineering Processor
Every 20 ticks do
    Input diamond from "diamond"
    Output to engineeringpress slots 2
End

Every 20 ticks do
    Input engineering_processor from engineeringprocessor
    Output to "output"
End

-- Concurrent Processor
Every 20 ticks do
    Input from entrocrystal
    Output to concurrentpress slots 2
End

Every 20 ticks do
    Input concurrent_processor from concurrentprocessor
    Output to "output"
End

-- Energy Processor
Every 20 ticks do
    Input from chargedredstonecrystal
    Output to energypress slots 2
End

Every 20 ticks do
    Input energy_processor from energyprocessor
    Output to "output"
End

 -- Accumulation Processor
Every 20 ticks do
    Input from skysteelingot
    Output to accumulationpress slots 2
End

Every 20 ticks do
    Input accumulation_processor from accumulationprocessor
    Output to "output"
End
